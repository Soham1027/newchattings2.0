{
    "functions": {
      "api/dynamic.py": {
        "runtime": "python3.12"
      }
    },
    "builds": [
      {
        "src": "api/dynamic.py",
        "use": "@vercel/python",
        "config": {
          "pythonVersion": "3.12.4"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "api/dynamic.py"
      }
    ]
  }
  